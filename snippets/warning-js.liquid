    	#dialogbox > div > #dialogboxfoot > #yesbutton{float:right;} 
</style>
<script type="text/javascript">
      if(typeof(Storage) !== "undefined") {
        if (sessionStorage.clickcount) {
           sessionStorage.clickcount = Number(sessionStorage.clickcount)+1;
        } else {
            sessionStorage.clickcount = 1;
        }}
        
        function CustomAlert(){
        this.render = function(){
        if(sessionStorage.clickcount == 1){
        sessionStorage.clickcount = Number(sessionStorage.clickcount)+1;
        
        var winW = window.innerWidth
        var winH = window.innerHeight
        var dialogoverlay = document.getElementById('dialogoverlay');
        var dialogobox = document.getElementById('dialogbox');
        dialogoverlay.style.display = "block";
        dialogoverlay.style.height = winH + "px";
        dialogbox.style.left = "37%";
        dialogbox.style.top = (winH/2)- (300*0.8) + "px";
        dialogbox.style.display = "block";
        document.getElementById('dialogboxhead').innerHTML = "AGE VERIFICATION";
        document.getElementById('dialogboxbody').innerHTML = 'Intended for sale to adults <hr/> <span style = "color:#81F781">18 years or older</span>.';
        document.getElementById('nobutton').innerHTML = '<input type="submit" value="18- LEAVE" style="font-size:1vw" onclick="Alert.no()"/>&nbsp;';
        document.getElementById('yesbutton').innerHTML = '<input type="submit" value="18+ ENTER" style="font-size:1vw" onclick="Alert.yes()"/>&nbsp;';
        }
        }
        this.yes = function(){
        document.getElementById('dialogbox').style.display = "none";
        document.getElementById('dialogoverlay').style.display = "none";
        }
        this.no = function(){
        document.location.replace("http://www.google.com");
        }
        }
  		
        //onload="Alert.render()" ;
        var Alert = new CustomAlert();
  		window.onload = Alert.render;
</script>